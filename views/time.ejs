<!Doctype html>
<html>
<head>
  <meta charset="utf-8">
  <title>時間割</title>
</head>
<body>
  <h2>時間割の一覧</h2>
  <table>
    <tr><th>ID</th><th>曜日</th><th>科目名</th></tr>
      <% let number = 0 %>
      <% for( let row of data ) { %>
        <tr><td><%= row.id %></td>
        <td><%= row.day %></td>
        <td><a href="/time/<%= number %>"><%= row.subject %></a></td></tr>
        <% number += 1 %>
      <% } %>
  </table>
  <a href="/time/create">追加</a>
</body>
</html>
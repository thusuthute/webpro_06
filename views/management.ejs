<!Doctype html>
<html>
<head>
  <meta charset="utf-8">
  <title>売上管理</title>
</head>
<body>
  <h2>売上管理の一覧</h2>
  <table>
    <tr><th>ID</th><th>JANコード</th><th>商品名</th></tr>
      <% let number = 0 %>
      <% for( let row of data ) { %>
        <tr><td><%= row.id %></td>
        <td><%= row.code %></td>
        <td><a href="/management/<%= number %>"><%= row.name %></a></td></tr>
        <% number += 1 %>
      <% } %>
  </table>
  <a href="/management/create">追加</a>
</body>
</html>